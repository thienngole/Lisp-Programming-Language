(defun untwin( lst)
  (cond ((null lst) ())
        ((equal (car lst) (car (cdr lst))) (cons (car(cdr lst)) (untwin (cdr(cdr lst))) ) )
        (t (cons (car lst) (untwin (cdr lst))))
   )
)

